{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 0 * * *"
        },
        {
            "path": "/api/cron",
            "schedule": "0 5 * * *"
        },
        {
            "path": "/api/cron",
            "schedule": "0 6 * * *"
        },
        {
            "path": "/api/cron",
            "schedule": "0 8 * * *"
        },
        {
            "path": "/api/cron",
            "schedule": "0 13 * * *"
        }
    ]
}